!
!
! OUTPUT OPTIONS
! ==============
	ntimepr = 20	! number of time steps per output
	iprint = 0	! 1 = print detailed debug info
	itimer = 1	! 1 = print timing info
!
!
! NUMERICAL METHOD OPTIONS
! ========================
	imask = 0	! 1 = zero out two-fluid terms near the boundaries
!
	integrator = 0	! 0 = Crank-Nicholson, 1 = BDF2

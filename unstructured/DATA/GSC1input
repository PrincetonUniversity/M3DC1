	hyperc = 0.0    ! hyper-viscosity   (applied to chi)
	hyperp = 0.0    ! hyper-pressure-diffusion
!
	dt = 1.00		! time step
	ntimemax = 100	! number of time steps
!
!
! MODEL OPTIONS
! =============
	linear = 1	! 0 = nonlinear; 1 = linear
	eqsubtract = 1	! 1 = subtract equilibrium in nonlinear simulations
      	numvar = 3	! 1 = 3-field; 2 = 4-field; 1 = 6-field
      	idens = 0	! 1 = advance density
	ipres = 0       ! 1 = include   pressure equation
	gyro = 0
!
	iconstflux = 0  ! 1 = conserve toroidal flux
        inocurrent_pol = 1  ! neumann condition on I
